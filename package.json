{
  "name": "shark-data-server",
  "version": "2.0.0",
  "description": "UDP视频流和MQTT数据发送模拟服务器",
  "main": "js/UDPserver.js",
  "scripts": {
    "start": "node js/UDPserver.js",
    "mqtt": "node js/mqtt-server.js",
    "mqtt-visual": "node js/mqtt-server-visual.js",
